{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": "sqs:*",
        "Resource": "*"
      },
      {
        "Effect": "Allow",
        "Action": "logs:*",
        "Resource": "*"
      },
      {
        "Effect": "Allow",
        "Action":"s3:*",
        "Resource":"*"
      },
      {
        "Effect": "Allow",
        "Action":"dynamodb:*",
        "Resource":"*"
      },
      {
        "Effect": "Allow",
        "Action":"kinesis:*",
        "Resource":"*"
      },
      {
        "Effect": "Allow",
        "Principal": {
          "Service": "apigateway.amazonaws.com"
        },
        "Action": "lambda:InvokeFunction",
        "Resource":"*"
      }
    ]
}